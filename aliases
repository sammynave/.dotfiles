# Zsh
alias reload=". ~/.zshrc && echo 'ZSH config reloaded from ~/.zshrc'"

# Vim
alias vi="vim"

# Git
alias gs="git status"

# Ruby
alias b="bundle"
alias be="b exec"
alias ber="be rake"

# MeYou Health
alias cuc="be spring cucumber --f progress"
alias cucdb="bundle exec rake parallel:create && bundle exec rake db:setup db:migrate db:seed && bundle exec rake parallel:prepare parallel:migrate parallel:seed"
alias cucp="be parallel_cucumber -o '--f progress'"
alias dcw="npm install && ember server --environment=development"
alias dcm="b && rails s -p 5000"
alias morning="ber update:env && ber fixtures:challenge"
alias morningp="RAILS_ENV=development parallel_test -e 'rake update:env fixtures:challenge'"
alias dbdump="/usr/local/Cellar/mysql/5.6.26/bin/mysqldump -u root meyouhealth_development > .dbbackup.sql"
alias dbload="/usr/local/Cellar/mysql/5.6.26/bin/mysql -u root meyouhealth_development < .dbbackup.sql"
alias s="be spring rspec"

